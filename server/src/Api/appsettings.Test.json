{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default":"Debug",
      "Override": {
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "JwtConfiguration": {
    "Secret": "JwtConfigurationJwtConfiguration"
  },
  "DatabaseConfiguration": {
    "Host": "localhost",
    "Port": "5432",
    "Database": "wordki-test",
    "User": "root",
    "Password": "changeme"
  },
  "ASPNETCORE_ENVIRONMENT": "Test"
}